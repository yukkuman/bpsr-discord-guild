{
  "name": "guild-chat-forwarder",
  "version": "0.1.0",
  "private": false,
  "type": "commonjs",
  "main": "src/main.js",
  "bin": {
    "guild-chat": "./src/main.js"
  },
  "scripts": {
    "start": "node ./src/main.js",
    "build:win": "pkg -t node18-win-x64 --output dist/bpsr-discord-guild.exe ."
  },
  "dependencies": {
    "axios": "^1.7.9",
    "cap": "^0.2.1",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  },
  "pkg": {
    "assets": [
      "node_modules/cap/build/Release/cap.node",
      "node_modules/axios/dist/**/*"
    ],
    "scripts": [
      "src/**/*.js"
    ]
  }
}
